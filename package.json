{
  "name": "@simon_he/runall",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.22.0",
  "description": "",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/runall#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/runall.git"
  },
  "bugs": "https://github.com/Simon-He95/runall/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "runall": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --minify",
    "dev": "npm run build -- --watch src",
    "run-all": "runall \"pnpm lint\"  \"pnpm i\"",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lazy-js-utils": "^0.0.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.7.4",
    "vitest": "^0.26.0"
  }
}
